  <div class="parallax2">
  <div class="row">
    <div class="col-xs-10 col-xs-offset-1">
        {{#each articles}}
        <div class="panel panel-default panel-table">
          <div class="panel-heading">
            <h3 class='panel-title text-center'>{{this.title}}</h3>
          </div>
          <div class="panel-body">
            <table class="table table-striped table-bordered table-list">
                <thead>
                    <tr>
                      <th>
                        <em class='glyphicon glyphicon-cloud-download'></em>
                      </th>
                      <th>
                        <span class='glyphicon glyphicon-eye-open'></span>
                        Quick Summary
                        </th>
                      <th>
                        <span class='glyphicon glyphicon-link'>
                        </span>
                        Full Article Link</th>
                    </tr>
                  </thead>
                  <tbody>
                    <tr class='tdata'>
                      <td align='center'>
                        <a href="#">
                          <button type="button" class="btn btn-success">Save</button>
                        </a>
                      </td>
                      <td>{{this.body}}</td>
                      <td>
                        <a href="{{this.link}}" target="_blank">{{this.link}}</a>
                        </td>
                    </tr>
                  </tbody>
              </table>
          </div>
        </div>
        <div class="parallax3">

        </div>
        {{/each}}
      </div>
    </div>

    </div>


  <div class="row">
    <div class="col-xs-12">
      <footer>
        <h3>ATX</h3>
          <h2>NEWS</h2>
          <p>Scraped from
            <a href="https://kxan.com/" target="_blank">KXAN.com</a>
          </p>
      </footer>
    </div>
  </div>

